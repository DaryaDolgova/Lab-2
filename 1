
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
double x1,x2,y1,y2,l,a,b;
cout<<"koordinata x1= "; cin>>x1;
cout<<"koordinata y1= "; cin>>y1;
cout<<"koordinata x2= "; cin>>x2;
cout<<"koordinata y2= "; cin>>y2;\
a=x2-x1;
b=y2-y1;
l=sqrt(a*a+b*b);
cout<<"dlina= "<<l<<endl;
return 0;
}
